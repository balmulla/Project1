<p>
    <table class="table">
  <thead>
    <tr>
      <th scope="col">Andrew ID</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>


      <th scope="col">Status</th>
      <th scope="col">Edit</th>
      <th scope="col">Delete</th>
    </tr>
  </thead>
  <tbody>
    <tr>
<% @students.each do |andrewid, firstname, lastname, status| %>        
      <th scope="row"><%= andrewid %></th>
      <td><%= firstname %></td>
      <td><%= lastname %></td>
      <td><%= status %></td>
      <td><a href="/students/<%= andrewid %>/edit" class="btn btn-info">edit</a></td>
      <td><a href = "/students/<%= andrewid %>/delete" class="btn btn-danger">delete</a></td>
    </tr>
    <% end %>
  </tbody>
</table>


</p>